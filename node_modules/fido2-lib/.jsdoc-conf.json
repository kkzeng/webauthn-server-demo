{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": "lib",
        "includePattern": "\\.js$",
        "excludePattern": "node_modules"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "systemName": "FIDO2 Library",
        "copyright": "Copyright 2018, Adam Powers",
        "template": "node_modules/docdash",
        "readme": "README.md",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "static": true,
        "sort": true
    }
}
